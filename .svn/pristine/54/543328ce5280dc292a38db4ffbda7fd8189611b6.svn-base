/* pages/enter/enter.wxss */
.main {
    width: 100vw;
    height: 100vh;
}
.container {
    margin: 162rpx auto;
    width: 690rpx;
    height: 1124rpx;
}
.welcome {
    margin: 124rpx auto 62rpx;
    width: 456rpx;
    height: 116rpx;
}
.welcome image{
    width: 100%;
    height: 100%;
}
.description{
    margin: 0 auto 110rpx;
    height: 53rpx;
    font-size: 50rpx;
    font-family: Source Han Sans CN;
    font-weight: 400;
    color: #1560AE;
}
.inpt-box {
    margin-bottom: 160rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    width: 575rpx;
    height: 262rpx;
  
}
.icon-main {
    
    display: flex;
    margin: 33rpx 0 18rpx 35rpx;
    width: 156rpx;
    height: 44rpx;
    border-right: 2rpx solid #FFFFFF;
}
.inpt {
    position: relative;
    display: flex;
    padding-top: 1rpx;
    width: 100%;
    height: 119rpx;
    
}
.intext {
    margin: 22rpx 0 15rpx 0;
    width: 350rpx;
    height: 67rpx;
   
}
.icon-ID {
    width: 44rpx;
    height: 44rpx;
}
.icon-ID image{
    width: 100%;
    height: 100%;
}
.icon-rig {
 margin-left: 20rpx;
height: 35rpx;
font-size: 30rpx;
font-family: Source Han Sans CN;
font-weight: 400;
color: #FFFFFF;
}
.abusloud {
    position: absolute;
    top: 100rpx;
    left: 35rpx;
    width: 506rpx;
    height: 2rpx;
    background-color: #fff;
}
.submit {
    line-height: 96rpx;
    width: 555rpx;
    height: 96rpx;
   
}